@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Capacitacion
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.Capacitacion.AsignacionProyectoModel
<div class="main-content view-wrap">   
      <div class="GmdBody-header main-title">
        <div class="pull-left">
            <h2 class="tituloCapacitacion"><strong><i class="fa fa-users"></i>@Html.Raw(AsignacionResource.EtiquetaTituloAsignacionPersonas)</strong></h2>
        </div>
        <div class="pull-right">
            <a id="btnRegresarAsignacionProyecto" class="nav_ajx btn-action-form tool pull-left">
                <span data-toggle="tooltip" data-placement="left" title="" data-original-title="List of Records">
                    <i class="listado_record"></i>
                </span>
            </a>
        </div>
        <div class="pull-right">
            <span class="box-type-record">
                <div class="row-line-table">
                    <div class="cell-line">
                        <label><strong>@Html.Raw(AsignacionResource.EtiquetaProyecto)</strong></label>
                    </div>
                    <div class="cell-line">
                        <div class="form-group">
                            <input type="text" id="hdnNombreProyecto" class="form-control" readonly="readonly" value="@ViewBag.NombreProyecto"/>
                            <input type="hidden" id="hdnCodigoProyecto" value="@ViewBag.CodigoProyecto"/>
                            <input type="hidden" id="hdnCodigoAsignacionProyecto" value="@ViewBag.CodigoAsignacionProyecto"/>
                        </div>                        
                    </div>
                </div>
            </span>
        </div>
    </div>    
      <div id="divRenderRecordSeccionCampo" class="tab_custom">
        <div class="content-body">
            <div class="filter-avanzado">
                <div class="form-group">
                    <div class="row pull-right">
                        <div class="col-md-6">
                            <button class="btn btn-primary" type="submit" id="btnAgregarPersona">@Html.Raw(AsignacionResource.EtiquetaAgregarParticipante)</button></div>
                        @*<div class="col-sm-4">
                            <div>
                                <button class="btn btn-default" type="submit" id="btnCargarPersonal">@Html.Raw(AsignacionResource.EtiquetaCargarPersonal)</button></div>
                        </div>*@
                    </div>

                </div>
            </div>
            <div class="title-interna">
                <span>@Html.Raw(AsignacionResource.GridListaProyectoPersona) </span>
            </div>
            <div class="table-grilla">
                <div id="divGrdResultadopPersonas"></div>
            </div>
        </div>
    </div>
</div>
