        ns('Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Models')
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Models.IngresoOtrosRegistrosModel = @(Html.Raw(Json.Encode(Model)))

    </script>

}
