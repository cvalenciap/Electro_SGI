        ns('Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosEstadoDocumento')
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosEstadoDocumento.Abierto = '@DatosConstantes.ParametrosEstadoDocumento.Abierto';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosEstadoDocumento.Cerrado = '@DatosConstantes.ParametrosEstadoDocumento.Cerrado';
