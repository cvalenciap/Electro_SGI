        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosEstadoDocumento.AnuladoPorGerencia = '@DatosConstantes.ParametrosEstadoDocumento.AnuladoPorGerencia'
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosEstadoDocumento.Reabierto = '@DatosConstantes.ParametrosEstadoDocumento.Reabierto'
