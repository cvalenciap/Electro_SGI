        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Actions.BuscarOtrosRegistros = '@Url.Action("BuscarOtrosRegistros", "BandejaOtrosRegistros", new { Area = "OtrosRegistros" })';
