@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Base
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Seguimiento
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Mensajes
@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@using Pe.ElectroPeru.SGI.Transversal.Core.Constante
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.Seguimiento.Accion.RecordAccionModel

<div class="pull-main-content" style="padding: 0">
<div class="main-content view-wrap">
    <div class="GmdBody-header main-title">
        <h2 class="pull-left" style="width: auto">
            <strong>@Html.Raw(AccionResource.EtiquetaTituloRevisionAccion)</strong></h2>
        <div class="pull-left">
            <span class="box-type-record">
                <div class="row-line-table">
                    <div class="cell-line">
                        <label>@Html.Raw(AccionResource.EtiquetaTipoRecord):</label>
                    </div>
                    <div class="cell-line">
                        <div class="form-group">
                            @Html.TextBox("txtTipoRecord", Model.RecordAccion.DescripcionTipoRecord, new { @class = "form-control", @style = "width:460px", @id = "txtTipoRecord", @readonly = "readonly", @disabled = "disabled" })
                        </div>
                    </div>
                    <div class="cell-line">
                        <label>@Html.Raw(AccionResource.EtiquetaNumeroRecord):</label>
                    </div>
                    <div class="cell-line">
                        <div class="form-group">
                            @Html.TextBox("txtNumeroRecord", Model.RecordAccion.NumeroRecord, new { @class = "form-control", @style = "width:225px", @id = "txtNumeroRecord", @readonly = "readonly", @disabled = "disabled" })
                        </div>
                    </div>
                </div>
            </span>
        </div>
        <div class="pull-right">
            @*<a id="btnRegresarBandejaAccionModal" class="btn nav_ajx btn-action-form tool pull-left">
                <span data-toggle="tooltip" data-placement="left" title="" data-original-title="@Html.Raw(AccionResource.EtiquetaListaAcciones)">
                    <i class="listado_record"></i>
                </span>
            </a>*@
        </div>
    </div>
    <div class="tab_custom">
        <div class="content-body">
            <div class="left-tabs">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist" id="cartTabs">
                    <li role="presentation" class="active">
                        <a id="lnkTabGeneral" href="#tabGeneral" class="top-tab" aria-controls="tabGeneral" role="tab" data-toggle="tab">@Html.Raw(RecordsResource.EtiquetaTabGeneral)</a>
                    </li>
                    @if (Model.RecordAccion.CodigoTipoRecord != DatosConstantes.TipoRecord.Incidente)
                    {
                        <li role="presentation">
                            <a id="lnkTabPersonaInvolucrada" href="#tabPersonaInvolucrada" class="top-tab" aria-controls="tabPersonaInvolucrada" role="tab" data-toggle="tab">@Html.Raw(RecordsResource.EtiquetaTabPersonaInvolucrada)</a>
                        </li>
                    }
                    <li role="presentation">
                        <a id="lnkTabAnexo" href="#tabAnexo" class="top-tab" aria-controls="tabAnexo" role="tab" data-toggle="tab">@Html.Raw(RecordsResource.EtiquetaTabAnexos)</a>
                    </li>
                    @if (Model.RecordAccion.CodigoTipoRecord != DatosConstantes.TipoRecord.Incidente)
                    {
                    <li role="presentation">
                        <a id="lnkTabFoto" href="#tabFoto" class="top-tab" aria-controls="tabFoto" role="tab" data-toggle="tab">@Html.Raw(RecordsResource.EtiquetaTabFotos)</a>
                    </li>
                    }
                    <li role="presentation">
                        <a id="lnkTabCierre" href="#tabCierre" class="top-tab" aria-controls="tabCierre" role="tab" data-toggle="tab">@Html.Raw(RecordsResource.EtiquetaTabCierre)</a>
                    </li>
                </ul>
                <!-- Cierra Nav tabs -->
            </div>
            <div class="right-tabs">
                <!-- Tab panes -->
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane fade in active" id="tabGeneral">
                        <div id="divRenderFichaGeneral">
                        </div>
                    </div>

                    @if (Model.RecordAccion.CodigoTipoRecord != DatosConstantes.TipoRecord.Incidente)
                    {
                        <div role="tabpanel" class="tab-pane fade in" id="tabPersonaInvolucrada">
                            <div role="tabpanel" class="tab-pane fade in" id="personaInvolucrada">
                                <div class="title-interna">
                                    <span>@Html.Raw(RecordsResource.EtiquetaPersonaInvolucrada)</span>
                                </div>
                                <div class="table-grilla">
                                    <div id="divGrdResultadoPersonaInvolucrada">
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(RecordsResource.EtiquetaPersonaInvolucradaOtros):</label>
                                            @Html.TextArea("txtPersonaInvolucradaOtros", Model.RecordAccion.PersonaInvolucradaOtros, new { id = "txtPersonaInvolucradaOtros", @class = "form-control", @cols = "10", @rows = "6", @disabled = "disabled", @readonly = "readonly" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                    <div role="tabpanel" class="tab-pane fade in" id="tabAnexo">
                        <div class="title-interna">
                            <span>@Html.Raw(RecordsResource.EtiquetaAppendix)</span>
                            @*<div class="acciones-form btn-group btn-group-table">
                                @if (Model.RecordAccion.CodigoEstadoRecord != null && Model.RecordAccion.CodigoEstadoRecord != DatosConstantes.EstadoRecord.Cerrado)
                                {
                                <button id="btnAgregarRecordAnexoApendice" type="button" class="btn-table text-line one" data-toggle="modal" data-target="#addFile">
                                    @Html.Raw(RecordsResource.EtiquetaAgregarArchivo)
                                </button>
                                }
                            </div>*@
                        </div>
                        <div class="table-grilla">
                            <div id="divGrdResultadoRecordAnexoApendice">
                            </div>
                        </div>
                    </div>

                    <div role="tabpanel" class="tab-pane fade in" id="tabFoto">
                        <div role="tabpanel" class="tab-pane fade in" id="foto">
                            <div class="title-interna">
                                <span>@Html.Raw(RecordsResource.EtiquetaFotos)</span>
                                @*<div class="acciones-form btn-group btn-group-table">
                                    @if (Model.RecordAccion.CodigoEstadoRecord != null && Model.RecordAccion.CodigoEstadoRecord != DatosConstantes.EstadoRecord.Cerrado)
                                    {
                                    <button id="btnAgregarRecordAnexoFoto" type="button" class="btn-table text-line one">
                                        @Html.Raw(RecordsResource.EtiquetaAgregarFoto)
                                    </button>
                                    }
                                </div>*@
                            </div>
                            <div class="table-grilla">
                                <div id="divGrdResultadoRecordAnexoFoto">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div role="tabpanel" class="tab-pane fade in" id="tabCierre">
                        <div role="tabpanel" class="tab-pane fade in" id="cierre">
                            <div class="title-interna">
                                <span>@Html.Raw(RecordsResource.EtiquetaTabCierre)</span>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label>@Html.Raw(CierreResource.EtiquetaComentario):</label>
                                        @Html.TextArea("txtComentarioCierreRecord", Model.RecordAccion.ComentarioCierre, new { id = "txtComentarioCierreRecord", @disabled = "disabled", @readonly = "readonly", @rows = "10", @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-3">
                                    @if (Model.RecordAccion.CodigoEstadoRecord != null && Model.RecordAccion.CodigoEstadoRecord != DatosConstantes.EstadoRecord.Cerrado)
                                    {
                                        Model.RecordAccion.FechaEstadoCierreString = string.Empty;
                                    }
                                    <div class="form-group">
                                        <label>@Html.Raw(CierreResource.EtiquetaFechaCierre):</label>
                                        @Html.TextBox("txtFechaCierreRecord", Model.RecordAccion.FechaEstadoCierreString, new { id = "txtFechaCierreRecord", @disabled = "disabled", @readonly = "readonly", @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-sm-9">
                                    <div class="form-group">
                                        <label>@Html.Raw(CierreResource.EtiquetaCerradoPor):</label>
                                        @Html.TextBox("txtNombreUsuarioCierre", Model.RecordAccion.NombreColaboradorCierre, new { id = "txtNombreUsuarioCierre", @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
    </div>

@section scripts
