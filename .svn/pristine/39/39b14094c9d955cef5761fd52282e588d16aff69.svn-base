    @Styles.Render("~/Components/SelectCustomCss")
    @Styles.Render("~/Components/FancyBoxCss")
    @RenderSection("css", required: false)
    @{
        var user = HttpGyMSessionContext.CurrentAccount();      
    }

</head>
<body class="skin-stracon bar-dark aside-closed aside-closed-complete show-ui">
    <!--<GmdWrap>-->
    <section class="wrap-ui">
        <!--<GmdHeader>-->
        <header class="header">
            <a class="button-nav" href="#" data-asideslide="button"><i class="fa fa-bars"></i></a>
            <div class="logo-data"></div>
            <h1 class="name-app">@Html.Raw(GenericoResource.NombreSistema)</h1>
            <div class="pull-right ">
                <div class="pull-left" style="background-color: #fff;">
                    <div class="combo-custom">
                        @Html.DropDownList("slcIdioma", (List<SelectListItem>)Session["ListaIdioma"], new { @class = "", @id = "slcIdioma" })
                    </div>
                </div>
                <div class="pull-left comp-btn-user btn-group">
                    <button id="btnGroupDrop2" type="button" class="btn dropdown-toggle" data-toggle="dropdown">
                        @if (user != null)
                        { 
                            @Html.Hidden("hdnUsuarioLogueo", user.Alias, new { id = "hdnUsuarioLogueo" })
                            <img src="@Url.Content("~/Theme/images/system/usuario.jpg")"  alt="@user.NombreCompleto" style="width:30px; height:30px" />
@*}*@
                            <span class="name">@user.NombreCompleto</span>
                            <span class="caret"></span>
                        }
                    </button>
                    <ul class="dropdown-menu dropdown-menu-right" role="menu" aria-labelledby="btnGroupDrop2">
                        <li class="first">
                            <a href="@Url.Action("Index", "LogOut", new { Area = "Base" })">@GenericoResource.BotonCerrarSesion</a>
                        </li>
                    </ul>
                </div>
            </div>

        </header>
        <!--</GmdHeader>-->
        <!--<GmdBody>-->
        <div id="DivPrincipal">
            <div class="wrap-content">
                <!--<main-aside>-->
                <aside class="main-aside" data-asideslide="slide">
                    <nav class="main-nav">
                        <div class="bl-slide" style="left: -202px"></div>
                        @*@if (Session["PortalStracon"] == null)
                        {
                            
                        }*@
                        <ul id="lstMenu"></ul>
                    </nav>
                </aside>
                <!--</main-aside>-->
                <!--<pull-main-content>-->
                <div class="pull-main-content">
                    <!--<main-content>-->
                    <div class="main-content">
                        @RenderBody()
                    </div>
                    <!--</main-content>-->
                </div>
            </div>
            <!--</pull-main-content>-->
        </div>
        <!--</GmdBody>-->
        <!--<gmd-footer>-->
        <footer class="lay-GmdFooter">
            <div class="row">
                <div class="col-sm-12">
                    <p class="pull-left">Todos los derechos reservados. Graña y Montero 2016 R.1.0</p>
                    <ul class="pull-right">
                        <li class="title">Powered by</li>
                        <li class="img"><a href="#">
                            <img src="@Url.Content("~/Theme/images/system/logo-gmd.png")" alt="GMD"></a></li>
                    </ul>
                </div>
            </div>
        </footer>
        <!--</gmd-footer>-->
        <div class="nav-UpPage upPage" data-element='btnUp'><span class="glyphicon glyphicon-chevron-up"></span></div>
    </section>
