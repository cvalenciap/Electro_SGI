                            @if (Model.InvestigacionInvolucrado.CodigoEstadoInvestigacion == DatosConstantes.EstadoInvestigacion.PorElaborar)
                            {
                                <button class="btn btn-primary" id="btnGrabarFatigaPersonaInvolucrada">@GenericoResource.BotonGrabar</button>
                            }
                        </div>                        
                    </form>
                </div>

                <div role="tabpanel" class="tab-pane fade in" id="tabOtrosInvolucrado">
                    <form id="frmOtrosDetallesColaborador" onsubmit="return false;">
                        <fieldset id="flsOtrosDetallesColaborador" @(Model.InvestigacionInvolucrado.CodigoEstadoInvestigacion != DatosConstantes.EstadoInvestigacion.PorElaborar ? "disabled=disabled" : null)>
                            <div class="row">
                                <div class="col-sm-12">
                                    <label>@Html.Raw(InvestigacionResource.EtiquetaInvolucra):</label>
                                    @Html.TextBox("txtComoSeInvolucroOtrosInvolucrado", Model.InvestigacionPersonaInvolucrada.ComoSeInvolucro, string.Empty, new { id = "txtComoSeInvolucroOtrosInvolucrado", maxlength = 100, @class = "form-control" })
                                    <div class="text-right">
                                        <label>@Html.Raw(string.Format(GenericoResource.EtiquetaMaximoCaracteres, 100))</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>@Html.Raw(InvestigacionResource.EtiquetaSufrioLesion): (*)</label>
                                        @Html.DropDownList("ddlSufrioLesionOtrosInvolucrado", Model.ListaSufrioLesion, new { @class = "form-control", @id = "ddlSufrioLesionOtrosInvolucrado", @validable = "required Validar" })
                                    </div>
                                </div>

                                <div class="col-sm-9">
                                    <div class="form-group">
                                        <label>@Html.Raw(InvestigacionResource.EtiquetaOtrosEstadoServicioIncidente):</label>
                                        @Html.DropDownList("ddlEstadoServicioIncidenteOtrosInvolucrado", Model.ListaEstadoServicioIncidente, new { @class = "form-control", @id = "ddlEstadoServicioIncidenteOtrosInvolucrado" })
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <div class="comp-Modal-footer text-center">
                            <button class="btn btn-default" id="btnCancelarOtrosPersonaInvolucrada">@GenericoResource.BotonCancelar</button>
                            @if (Model.InvestigacionInvolucrado.CodigoEstadoInvestigacion == DatosConstantes.EstadoInvestigacion.PorElaborar)
                            {
                                <button class="btn btn-primary" id="btnGrabarOtrosPersonaInvolucrada">@GenericoResource.BotonGrabar</button>
                            }
                        </div>                        
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    ns('Pe.ElectroPeru.SGI.Presentacion.Seguimiento.Investigacion.Models')
    Pe.ElectroPeru.SGI.Presentacion.Seguimiento.Investigacion.Models.InvestigacionInvolucradoModel = @(Html.Raw(Json.Encode(Model)))
</script>
