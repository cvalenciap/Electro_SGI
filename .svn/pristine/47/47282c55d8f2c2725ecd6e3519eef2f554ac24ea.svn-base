        ns('Pe.ElectroPeru.SGI.Presentacion.Capacitacion.Mantenimiento.Actions')
        Pe.ElectroPeru.SGI.Presentacion.Capacitacion.Mantenimiento.Actions.BandejaFamiliaEquipo = '@Url.Action("Index", "Configuracion/FamiliaEquipo", new { Area = "Capacitacion" })';
        

        ns('Pe.ElectroPeru.SGI.Presentacion.Capacitacion.FormularioSeleccionarEquipos.Actions')
        Pe.ElectroPeru.SGI.Presentacion.Capacitacion.FormularioSeleccionarEquipos.Actions.BuscarProcesos = '@Url.Action("BuscarAsignacionProcesosPorProyecto", "Asignacion", new { Area = "Capacitacion" })';
        Pe.ElectroPeru.SGI.Presentacion.Capacitacion.FormularioSeleccionarEquipos.Actions.FormularioAgregarProcesoModal = '@Url.Action("FormularioAgregarProcesoModal", "Asignacion", new { Area = "Capacitacion" })';
    
        </script>

     @CustomHtmlHelper.RenderViewJs("capacitacionasignacionformularioagregarprocesomodal");
    @CustomHtmlHelper.RenderViewJs("capacitacionasignacionformularioseleccionarsubprocesomodal");
}