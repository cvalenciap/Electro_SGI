        Pe.ElectroPeru.SGI.Presentacion.Capacitacion.FormularioProceso.Actions.RegistrarAsignacionProceso = '@Url.Action("RegistrarAsignacionProceso", "Asignacion", new { Area = "Capacitacion" })';
