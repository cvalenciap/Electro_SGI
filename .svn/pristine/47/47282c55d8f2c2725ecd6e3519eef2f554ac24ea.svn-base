        Pe.ElectroPeru.SGI.Presentacion.Capacitacion.FormularioProceso.Actions.BuscarAsignacionProcesosPorProyecto = '@Url.Action("BuscarAsignacionProcesosPorProyecto", "Asignacion", new { Area = "Capacitacion" })';
