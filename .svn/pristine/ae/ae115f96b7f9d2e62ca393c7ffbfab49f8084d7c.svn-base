@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Base
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.OtrosRegistros
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Seguimiento
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Mensajes
@using Pe.ElectroPeru.SGI.Transversal.Core.Constante
@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.OtrosRegistros.IngresoOtrosRegistros.OtrosRegistrosModel
<div class="main-content view-wrap">
    <div class="GmdBody-header main-title">
        <h2 class="pull-left">
            <strong>@OtrosRegistrosResource.EtiquetaTituloCierreOtrosRegistros</strong>
        </h2>
    </div>  
    <div class="content-body">
        <div class="filter-avanzado">
            <div class="row">
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaTipoRegistro):</label>
                        @Html.DropDownList("slcTipoRegistro", Model.ListaTipoRegistro, new { @class = "form-control", @id = "slcTipoRegistro" })
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaPais):</label>
                        @Html.DropDownList("slcPais", Model.ListaPais, new { @class = "form-control", @id = "slcPais" })
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaEmpresa):</label>
                        @Html.DropDownList("slcEmpresaPais", Model.ListaEmpresa, new { @class = "form-control", @id = "slcEmpresaPais" })
                    </div>
                </div>
              
            </div>
            <div class="row">
                  <div class="col-sm-4">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaProyecto):</label>
                        @Html.DropDownList("slcProyecto", Model.ListaProyectos, new { @class = "form-control", @id = "slcProyecto" })
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaFechaDesde):</label>
                        <input id="dtpFechaEjecucionDesde" class="form-control">
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaFechaHasta):</label>
                        <input id="dtpFechaEjecucionHasta" class="form-control">
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label>@Html.Raw(OtrosRegistrosResource.GridEstadoDocumento):</label>
                        @Html.DropDownList("slcEstadoDocumento", Model.ListaEstadoDocumento, new { @class = "form-control", @id = "slcEstadoDocumento" })
                    </div>
                </div>
            </div>
        </div>
        <div class="box-acciones-foot filter">
            <button class="btn btn-default" id="btnLimpiar">@Html.Raw(GenericoResource.EtiquetaLimpiar)</button>
            <button class="btn btn-primary" id="btnBuscar">@Html.Raw(GenericoResource.EtiquetaBuscar)</button>
        </div>
        <div class="row">
            <div class="col-sm-6">
                <h4>@Html.Raw(GenericoResource.EtiquetaResultadosBusqueda)</h4>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                @* <div class="table-responsive">*@
                <div class="table-grilla">
                    <div id="divGrdResultOtrosRegistros">
                    </div>
                </div>
                @*</div>*@
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        ns('Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource')
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridNumeroTipoRegistro = '@Html.Raw(OtrosRegistrosResource.GridNumeroTipoRegistro)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridTipoRegistro = '@Html.Raw(OtrosRegistrosResource.GridTipoRegistro)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridEmpresa = '@Html.Raw(OtrosRegistrosResource.GridEmpresa)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridNombreComite = '@Html.Raw(OtrosRegistrosResource.GridNombreComite)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridPais = '@Html.Raw(OtrosRegistrosResource.GridPais)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridProyecto = '@Html.Raw(OtrosRegistrosResource.GridProyecto)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridFechaEjecucion = '@Html.Raw(OtrosRegistrosResource.GridFechaEjecucion)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridTituloRegistro = '@Html.Raw(OtrosRegistrosResource.GridTituloRegistro)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridEstadoIngresoDocumento = '@Html.Raw(OtrosRegistrosResource.GridEstadoIngresoDocumento)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridEstadoDocumento = '@Html.Raw(OtrosRegistrosResource.GridEstadoDocumento)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridEstadoMinimoAccion = '@Html.Raw(OtrosRegistrosResource.GridEstadoMinimoAccion)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.GridAlerta = '@Html.Raw(OtrosRegistrosResource.GridAlerta)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.EtiquetaAlertaRequiereCierre = '@Html.Raw(OtrosRegistrosResource.EtiquetaAlertaRequiereCierre)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.EtiquetaAlertaNoRequiereCierre = '@Html.Raw(OtrosRegistrosResource.EtiquetaAlertaNoRequiereCierre)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.EtiquetaAlertaAnuladaPorGerencia = '@Html.Raw(OtrosRegistrosResource.EtiquetaAlertaAnuladaPorGerencia)';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Resource.EtiquetaAlertaCombinacionNoDefinida = '@Html.Raw(OtrosRegistrosResource.EtiquetaAlertaCombinacionNoDefinida)';

        ns('Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Actions')
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Actions.BuscarBandejaCierreOtrosRegistros = '@Url.Action("BuscarCierreOtrosRegistros", "BandejaCierreOtrosRegistros", new { Area = "OtrosRegistros" })';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.Actions.FormularioCierreOtrosRegistros = '@Url.Action("Index", "CierreOtrosRegistros", new { Area = "OtrosRegistros" })';

        ns('Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosTiposAlertas')
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosTiposAlertas.RequiereCierre = '@DatosConstantes.ParametrosTiposAlertas.RequiereCierre';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosTiposAlertas.NoRequiereCierre = '@DatosConstantes.ParametrosTiposAlertas.NoRequiereCierre';
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosTiposAlertas.AnuladaPorGerencia = '@DatosConstantes.ParametrosTiposAlertas.AnuladaPorGerencia'
        Pe.ElectroPeru.SGI.Presentacion.OtrosRegistros.Index.DatosConstantes.ParametrosTiposAlertas.CombinacionNoDefinida = '@DatosConstantes.ParametrosTiposAlertas.CombinacionNoDefinida'

    </script>
}
