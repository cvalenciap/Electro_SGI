                                <button class="btn btn-primary" @(Model.ComiteSSoma.CodigoComiteSSoma.HasValue ? "" : "hidden") id="btnGuardarHallazgo">@Html.Raw(GenericoResource.BotonGuardar)</button>
                            </div>                                          
                        }
