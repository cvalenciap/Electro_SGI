/*----header*/
/*******************************/
.header {
    height: 53px;
    border-top: 1px solid #dddedf;
    border-bottom: 1px solid #dddedf;
    background: #3e3d3d;
    position: fixed;
    width: 100%;
    z-index: 11;
}

.logo {
    display: block;
}

.header .logo {
    display: block;
    float: left;
    margin-top: 12px;
    margin-left: 22px;
    min-width: 102px;
    border-right: 1px solid #d9d9d9;
}

.header h1 {
    float: left;
    margin: 13px;
    font-weight: bold;
    color: #676767;
    font-size: 16px;
    text-transform: uppercase;
    margin-top: 18px;
}
/*----footer*/
/*******************************/
footer {
    background: #2364a6;
    color: #fff;
    font-size: 12px;
    height: 52px;
    position: fixed;
    bottom: 0px;
    width: 100%;
    z-index: 100;
}

    footer ul {
        padding: 0px;
        margin: 0px;
    }

    footer a,
    footer p {
        color: #fff;
        font-size: 12px;
    }

    footer li {
        list-style: none;
    }

    footer .pull-left {
        margin: 18px 0 0 15px;
    }

    footer .pull-right {
        margin: 18px 15px 0 0;
    }

        footer .pull-right li {
            display: inline-block;
        }

        footer .pull-right .title {
            margin-right: 10px;
        }

        footer .pull-right a {
            display: block;
            position: relative;
            top: -2px;
        }
/*----main-aside*/
/*******************************/
.button-nav {
    display: block;
    height: 52px;
    width: 58px;
    background: #2364a6;
    border: none;
    float: left;
    text-align: center;
    color: #fff;
}

    .button-nav .fa-bars {
        font-size: 21px;
        margin: 15px 0 0;
        color: #fff;
    }

    .button-nav.active .fa-bars:before {
        content: "\f00d";
    }

/*----main-nav*/
.main-nav {
}

    .main-nav ul {
        margin: 0px;
        padding: 0px;
    }

        .main-nav ul li {
            list-style: none;
        }

    .main-nav .bl-slide {
        height: 2000px;
        background: #2364a6;
        position: fixed;
        width: 260px;
    }

    .main-nav > ul > li {
        position: relative;
    }

        .main-nav > ul > li > a {
            font-weight: bold;
        }

    .main-nav a {
        display: block;
        font-size: 12px;
        color: #ffffff;
        min-height: 53px;
    }

    .main-nav .wrap-icon {
        width: 58px;
        height: 100%;
        position: absolute;
        top: 0px;
        left: 0px;
        text-align: center;
        line-height: 54px;
        background: #2a2a2c;
        z-index: 9999;
        font-size: 17px;
    }

    .main-nav .wrap-text {
        display: block;
        padding: 18px 35px 18px 14px;
        position: relative;
        background: #2364a6;
        margin-left: 58px;
        z-index: 1;
        width: 202px;
    }

        .main-nav .wrap-text a {
            display: inline;
            color: #fff;
            margin-left: 0px!important;
            font-size: 13px;
        }

    .main-nav .wrap-text-inner {
        padding: 18px 35px 18px 14px;
        display: block;
        margin-left: 58px;
        background: #2364a6;
    }

    .main-nav .menu-dropdown .fa-bars {
        color: #636363;
        position: absolute;
        font-size: 14px;
        right: 11px;
        top: 50%;
        margin-top: -7px;
    }

    .main-nav .menu-dropdown ul {
        display: none;
    }

    .main-nav .menu-dropdown li.title {
        padding: 18px 35px 18px 14px;
        color: #fff;
        margin-left: 0px!important;
    }

        .main-nav .menu-dropdown li.title a {
            display: inline;
            color: #fff;
            margin-left: 0px!important;
            font-size: 13px;
        }

    .main-nav .menu-dropdown:before {
        content: '';
        position: absolute;
        width: 4px;
        height: 100%;
        background: #ff8e00;
        z-index: 99999;
    }

    .main-nav li:hover > a,
    .main-nav li.active > a,
    .main-nav li.active .fa-bars,
    .main-nav li:hover .fa-bars,
    .main-nav a.active {
        color: #fff;
        background-color:transparent;
    }

    /*.main-nav .menu-dropdown.active:before, .dropdown.open .dropdown-menu li:before {
        background: #3763b0;
    }*/

/*----.aside-open*/
.aside-open .sub-menu .title {
    display: none;
}

.aside-open-complete .main-aside {
    width: 260px;
}

.aside-open-complete .main-nav {
    position: fixed;
    bottom: 52px;
    top: 52px;
    background: #2a2a2c;
    overflow: hidden;
    overflow-y: auto;
}

.main-aside {
    position: fixed;
    -webkit-overflow-scrolling: touch;
    opacity: .95;
    -webkit-transition: all .5s ease-in;
    -o-transition: all .5s ease-in;
    -moz-transition: all .5s ease-in;
    -ms-transition: all .5s ease-in;
    transition: all .5s ease-in;
    z-index: 100;
    float: left;
    float: left;
    position: fixed;
    margin-right: -100%;
    top: 52px;
    bottom: 52px;
}
/*----.aside-closed*/

.aside-closed .main-nav > ul > li:hover .sub-menu {
    color: #fff;
    overflow: hidden;
    overflow-y: auto;
    margin: 0px;
    padding: 0px;
}

.aside-closed .main-nav > ul > li .sub-menu li {
    background: #2364a6;
    margin-left: 2px;
}

.aside-closed-complete .main-nav ul ul {
    /* position: absolute;
    top: 0px;
    left: 58px;
    width: 202px;*/
    /*-webkit-box-shadow:5px 5px rgba(44, 53, 66, 0.2);  se quito la sombra del menu 
	box-shadow:5px 5px rgba(44, 53, 66, 0.2);
	-moz-box-shadow:5px 5px rgba(44, 53, 66, 0.2);*/
    position: fixed;
    top: 53px;
    left: 58px;
    width: 202px;
    bottom: 52px;
    background-color: #2364a6;
}

    .aside-closed-complete .main-nav ul ul.oneItem {
        position: absolute;
        top: 0px;
        bottom: inherit;
    }

.aside-closed-complete .main-nav > ul > li:hover ul, .aside-closed-complete .main-nav > ul > li:hover ul .text {
    display: block!important;
}

.aside-closed-complete .main-nav .wrap-text {
    margin-left: 0px;
    width: auto;
    padding: 0px;
}

.aside-closed-complete .main-nav .wrap-text-inner {
    margin-left: 0px;
}

.aside-closed-complete .main-aside {
    width: 58px;
}

.aside-closed .sub-menu .title {
    display: none;
}

.aside-closed-complete .sub-menu .title {
    display: block;
}

.aside-closed .main-nav > ul > li:hover > a .wrap-text,
.aside-closed .main-nav > ul > li:hover > a .wrap-icon,
.aside-closed .main-nav > ul > li.active > a .wrap-text .aside-closed .main-nav > ul > li.active > a .wrap-icon {
    background: #373738;
    color: #fff;
}



.GmdBody-header {
    border-bottom: 1px solid #dedede;
