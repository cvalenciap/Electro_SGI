@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@using Pe.ElectroPeru.SGI.Transversal.Core.Constante
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.Seguimiento.Records.RecordsModel

<div class="main-content view-wrap">
    <div class="GmdBody-header main-title">
        <h2 class="pull-left">
            <div>
                <label>
                    <strong>@Html.Raw(RecordsResource.EtiquetaNuevoRegistroRecord)</strong>
                </label>
            </div>
        </h2>
        <div class="pull-left">
            <span class="box-type-record">
                <div class="row-line-table">
                    <div class="cell-line">
                        <label>@Html.Raw(RecordsResource.EtiquetaTipoRecord):</label>
                    </div>
                    <div class="cell-line">
                        <div class="form-group">
                            @*@if (!Model.Record.CodigoRecord.HasValue)
                            {
                                @Html.DropDownList("slcTipoRecord", Model.ListaTipoRecord, new { @class = "form-control", @id = "slcTipoRecord" })
                            }
                            else
                            {
                                @Html.TextBox("txtTipoRecord", Model.Record.DescripcionTipoRecord, new { @class = "form-control", @style = "width:460px", @id = "txtTipoRecord", @readonly = "readonly", @disabled = "disabled" })
                            }*@
                            @Html.TextBox("txtTipoRecord", Model.Record.DescripcionTipoRecord, new { @class = "form-control", @style = "width:460px", @id = "txtTipoRecord", @readonly = "readonly", @disabled = "disabled" })
                        </div>
                    </div>
                    <div class="cell-line">
                        <label>@Html.Raw(RecordsResource.EtiquetaNumeroRecord):</label>
                    </div>
                    <div class="cell-line">
                        <div class="form-group">
                            @Html.TextBox("txtNumeroRecord", Model.Record.NumeroRecord, new { @class = "form-control", @style = "width:225px", @id = "txtNumeroRecord", @readonly = "readonly", @disabled = "disabled" })
                        </div>
                    </div>
                </div>
            </span>
        </div>
        <div class="pull-right">
            <a id="btnRegresarBandejaRecord" class="btn nav_ajx btn-action-form tool pull-left">
                <span data-toggle="tooltip" data-placement="left" title="" data-original-title="List of Records">
                    <i class="listado_record"></i>
                </span>
            </a>
        </div>
    </div>
    <div id="divRenderRecordSeccionCampo" class="tab_custom">
    </div>
</div>

