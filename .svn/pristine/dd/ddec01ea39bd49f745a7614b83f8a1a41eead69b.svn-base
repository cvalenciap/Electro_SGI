<div class="GmdBody-header">
    <h2>@ResponsableResource.EtiquetaTituloPrincipal</h2>
</div>
<div class="GmdBody-body">
    <form id="frmResponsableRegistrar">
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group">
                    <label>@ResponsableResource.EtiquetaNombreCompleto</label>
                    @Html.TextBox("txtNombreCompleto", string.Empty, new { @id = "txtNombreCompleto", @class = "form-control", @maxlength = "50", onkeyup = "Pe.GMD.UI.Web.Components.Util.ToUpperCase(event)" })
                </div>
                <div class="form-group">
                    <input id="cboxSolicitante" type="checkbox" />
                    <label>@ResponsableResource.EtiquetaSolicitante</label>
                    @*@Html.TextBox("txtNombreCompleto", string.Empty, new { @id = "txtNombreCompleto", @class = "form-control", @maxlength = "50", onkeyup = "Pe.GMD.UI.Web.Components.Util.ToUpperCase(event)" })*@
                </div>
                <div class="form-group">
                    <input id="cboxAprobador" type="checkbox" />
                    <label>@ResponsableResource.EtiquetaAprobador</label>
                    @*@Html.TextBox("txtNombreCompleto", string.Empty, new { @id = "txtNombreCompleto", @class = "form-control", @maxlength = "50", onkeyup = "Pe.GMD.UI.Web.Components.Util.ToUpperCase(event)" })*@
            </div>          
            </div>          

        </div>
        
        
    </form>
    <div class="row">
        <div class="col-sm-12">
            <div class="form-group">
                <button id="btnLimpiar" class="btn btn-info" type="reset">@GenericoResource.BotonLimpiar</button>
                <button id="btnBuscar" class="btn btn-default" type="button">@GenericoResource.BotonMostrar</button>
            </div>
        </div>
    </div>
    <hr />
    <div>
        <div class="row">
            <div class="col-sm-6">
                <h3>@GenericoResource.EtiquetaResultadosBusqueda</h3>
            </div>
            <div class="col-sm-6 text-right">
                <button id="btnAgregar" class="btn btn-primary">@GenericoResource.BotonAñadir</button>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 align-left">
                <div id="divGrdResult">
                </div>
            </div>
        </div>
        @*        <div class="row">
            <div class="col-sm-12">
        <div class="table-responsive">
            <div id="divGrdResult">
            </div>
        </div>        
            </div>
        </div>*@
    </div>
</div>

