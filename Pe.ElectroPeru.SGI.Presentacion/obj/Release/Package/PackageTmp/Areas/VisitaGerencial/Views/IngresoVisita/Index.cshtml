@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Base
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.ActuaPositivo
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.VisitaGerencial
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Seguimiento
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Mensajes
@using Pe.ElectroPeru.SGI.Transversal.Core.Constante
@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.VisitaGerencial.IngresoVisita.IngresoVisitaModel
<div class="main-content view-wrap">
    <div class="GmdBody-header main-title">
        <h2 class="pull-left">
            <strong>@VisitaGerencialResource.EtiquetaBandejaVisitaGerencial</strong>
        </h2>
    </div>
    <div class="content-body">
        <div class="filter-avanzado">
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaProyecto):</label>
                        @Html.DropDownList("slcProyecto", Model.ListaProyectos, new { @class = "form-control", @id = "slcProyecto" })
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaFechaDesde):</label>
                        <input id="dtpFechaVisitaGerencialDesde" class="form-control">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaFechaHasta):</label>
                        <input id="dtpFechaVisitaGerencialHasta" class="form-control">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaEstadoVisita):</label>
                        @Html.DropDownList("slcEstadoIngresoDocumento", Model.ListaEstadoIngresoDocumento, new { @class = "form-control", @id = "slcEstadoIngresoDocumento" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaEstadoIngresoPlanAccion):</label>
                        @Html.DropDownList("slcEstadoIngresoPlanAccion", Model.ListaEstadoPlanAccion, new { @class = "form-control", @id = "slcEstadoIngresoPlanAccion" })
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaEstadoProcesoPlanAccion):</label>
                        @Html.DropDownList("slcEstadoProcesoPlanAccion", Model.ListaEstadoProcesoPlanAccion, new { @class = "form-control", @id = "slcEstadoProcesoPlanAccion" })
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaEstadoDocumento):</label>
                        @Html.DropDownList("slcEstadoDocumento", Model.ListaEstadoDocumento, new { @class = "form-control", @id = "slcEstadoDocumento" })
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label>@Html.Raw(VisitaGerencialResource.EtiquetaEstadoMinimoAccion):</label>
                        @Html.DropDownList("slcEstadoMinimoAccion", Model.ListaEstadoMinimoAccion, new { @class = "form-control", @id = "slcEstadoMinimoAccion" })
                    </div>
                </div>
            </div>
        </div>
        <div class="box-acciones-foot filter">
            <button class="btn btn-default" id="btnLimpiar">@Html.Raw(RecordsResource.EtiquetaLimpiar)</button>
            <button class="btn btn-primary" id="btnBuscar">@Html.Raw(RecordsResource.EtiquetaBuscar)</button>
        </div>
        <div class="row">
            <div class="col-sm-6">
                <h4>@Html.Raw(GenericoResource.EtiquetaResultadosBusqueda)</h4>
            </div>
            <div class="col-sm-6 text-right">
                <button id="btnDescargarExcel" class="btn btn-primary">@ActuaPositivoResource.BtnDescargarExcel</button>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="table-responsive">
                    <div class="table-grilla">
                        <div id="divGrdResultVisitaGerencial">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
