        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.ConfirmacionGuardarReabrir = '@Html.Raw(VisitaGerencialResource.ConfirmacionGuardarReabrir)';
