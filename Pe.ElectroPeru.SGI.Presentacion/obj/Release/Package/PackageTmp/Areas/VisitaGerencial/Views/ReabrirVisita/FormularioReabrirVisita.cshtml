                        @Html.TextBox("txtComentario", Model.VisitaGerencial.Comentario, new { id = "txtComentario", @class = "form-control", @validable = "required Validar" })
                    </div>
                </div>
            </div>
        </div>
        <div class="box-acciones-foot filter">  
            <button class="btn btn-primary" id="btnReabrirVisita">@Html.Raw(VisitaGerencialResource.EtiquetaBotonReabrirVisita)</button>

        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="table-responsive">
                    <div class="table-grilla">
                        <div id="divGrdResult">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        ns('Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.')
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.GridFechaPlanteada = '@Html.Raw(VisitaGerencialResource.GridFechaPlanteada)';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.GridPlanteaAccion = '@Html.Raw(VisitaGerencialResource.GridPlanteaAccion)';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.GridFechaVencimiento = '@Html.Raw(VisitaGerencialResource.GridFechaVencimiento)';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.GridEjecutaAccion = '@Html.Raw(VisitaGerencialResource.GridEjecutaAccion)';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.GridDescripcionHallazgo = '@Html.Raw(VisitaGerencialResource.GridDescripcionCortaHallazgo)';
