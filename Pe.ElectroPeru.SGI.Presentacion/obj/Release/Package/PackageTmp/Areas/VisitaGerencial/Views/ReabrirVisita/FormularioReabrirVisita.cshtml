        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.EtiquetaEstadoNoCerradoVisita  = '@Html.Raw(VisitaGerencialResource.EtiquetaEstadoNoCerradoVisita)';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Resource.EtiquetaSinComentario = '@Html.Raw(SeguimientoResource.EtiquetaSinComentario)';

        ns('Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.DatosConstantes.ParametrosEstadoDocumento')
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.DatosConstantes.ParametrosEstadoDocumento.Cerrado = '@DatosConstantes.ParametrosEstadoDocumento.Cerrado';
        
        ns('Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Actions')
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Actions.ReabrirVisitaGerencial = '@Url.Action("ReabrirVisitaGerencial", "ReabrirVisita", new { Area = "VisitaGerencial" })';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Actions.BuscarCierreBandeja =   '@Url.Action("BuscarCierreBandeja", "ReabrirVisita", new { Area = "VisitaGerencial" })';
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.ReabrirVisitaIndex = '@Url.Action("Index", "ReabrirVisita", new { Area = "VisitaGerencial" })';
    
        ns('Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Models')
        Pe.ElectroPeru.SGI.Presentacion.VisitaGerencial.ReabrirVisita.FormularioReabrirVisita.Models.ReabrirVisitaModel = @(Html.Raw(Json.Encode(Model)))

