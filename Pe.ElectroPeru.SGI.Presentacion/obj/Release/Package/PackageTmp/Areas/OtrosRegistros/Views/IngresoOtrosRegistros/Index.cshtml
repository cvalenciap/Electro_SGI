                                                            <button class="btn-form" id="btnBuscarColaboradorParticipante" type="button">....</button>
                                                        </div>
                                                    </div>

                                                    @Html.TextBox("txtNombreColaboradorInvitadoParticipante", Model.OtrosRegistrosParticipante.NombreCompletoColaboradorParticipante, Model.OtrosRegistrosParticipante.CodigoColaboradorParticipante.HasValue ? (object)new { id = "txtNombreColaboradorInvitadoParticipante", @class = "form-control", @Styles = "Display:none", @validable = "required Validar", } : new { id = "txtNombreColaboradorInvitadoParticipante", @class = "form-control", @validable = "required Validar" })

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaTipoDocumento): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistrosParticipante.CodigoTipoDocumento, Model.ListaTipoDocumento, Model.OtrosRegistrosParticipante.CodigoOtrosRegistrosParticipante.HasValue ? (object)new { id = "slcTipoDocumento", @class = "form-control", @validable = "required Validar" } : new { id = "slcTipoDocumento", @class = "form-control", @validable = "required Validar", })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaNumeroDocumento): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.TextBox("txtNumeroDocumento", Model.OtrosRegistrosParticipante.NumeroDocumento, Model.OtrosRegistrosParticipante.CodigoOtrosRegistrosParticipante.HasValue ? (object)new { id = "txtNumeroDocumento", @class = "form-control", @validable = "required Validar", @maxlength = "12" } : new { id = "txtNumeroDocumento", @class = "form-control", @maxlength = "12", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaApellidoPaterno): (*)</label>
                                            <div class="row">
