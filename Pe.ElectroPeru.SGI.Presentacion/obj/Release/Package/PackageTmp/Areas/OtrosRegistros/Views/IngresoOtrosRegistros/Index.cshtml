                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaApellidoMaterno): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.TextBox("txtApellidoMaterno", Model.OtrosRegistrosParticipante.ApellidoMaterno, Model.OtrosRegistrosParticipante.CodigoColaboradorParticipante.HasValue ? (object)new { id = "txtApellidoMaterno", @class = "form-control", @validable = "required Validar", @disabled = "disabled", @maxlength = "50" } : new { id = "txtApellidoMaterno", @class = "form-control", @maxlength = "50", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaCargoEmpresa): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
