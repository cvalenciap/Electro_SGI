                                                        @Html.Hidden("hdnCodigoColaboradorParticipante", Model.OtrosRegistrosParticipante.CodigoColaboradorParticipante, new { ïd = "hdnCodigoColaboradorParticipante" })
                                                        @Html.TextBox("txtNombreColaboradorParticipante", Model.OtrosRegistrosParticipante.NombreCompletoColaboradorParticipante, Model.OtrosRegistrosParticipante.CodigoColaboradorParticipante.HasValue ? (object)new { id = "txtNombreColaboradorParticipante", @class = "form-control", @disabled = "disabled", @validable = "required Validar" } : new { id = "txtNombreColaboradorParticipante", @class = "form-control", @disabled = "disabled", @validable = "required Validar" })
