@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Seguimiento
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.OtrosRegistros
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Mensajes
@using Pe.ElectroPeru.SGI.Presentacion.Recursos.Base
@using Pe.ElectroPeru.SGI.Presentacion.Core.Helpers
@using Pe.ElectroPeru.SGI.Transversal.Core.Constante
@model Pe.ElectroPeru.SGI.Presentacion.Core.ViewModel.OtrosRegistros.IngresoOtrosRegistros.OtrosRegistrosModel
<div class="main-content view-wrap">
    <div class="GmdBody-header main-title">
        <div class="pull-left">
            <span class="box-type-record">
                <div class="row-line-table">
                    <div class="cell-line">
                        <label><strong>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloOtrosRegistros):</strong></label>
                    </div>
                    <div class="cell-line">
                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaNroRegistro):</label>
                    </div>
                    <div class="cell-line">
                        @Html.TextBox("txtNumeroTipoRegistro", Model.OtrosRegistros.NumeroOtrosRegistros, new { id = "txtNumeroTipoRegistro", @class = "form-control", @disabled = "disabled" })
                    </div>
                </div>
            </span>
        </div>
        <div class="pull-right">
            <a id="btnRegresarBandejaInvestigacion" class="btn nav_ajx btn-action-form tool pull-left">
                <span data-toggle="tooltip" data-placement="left" title="" data-original-title="@Html.Raw(AccionResource.EtiquetaListaAcciones)">
                    <i class="listado_record"></i>
                </span>
            </a>
        </div>
    </div>
    <div class="tab_custom">
        <div class="content-body">
            <div class="left-tabs">
                <ul class="nav nav-tabs" role="tablist" id="cartTabsFormularioIngresoOtrosRegistros">
                    <li role="presentation" class="active validateOtrosRegistros">
                        <a href="#tabGeneral" class="top-tab" aria-controls="tabGeneral" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabGeneral)</a>
                    </li>
                    <li role="presentation" class="validateOtrosRegistros">
                        <a href="#tabHallazgo" class="top-tab" aria-controls="tabHallazgo" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabHallazgo)</a>
                    </li>
                    <li role="presentation" class="validateOtrosRegistros">
                        <a href="#tabAccion" class="top-tab" aria-controls="tabAccion" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabAcciones)</a>
                    </li>
                    <li role="presentation" class="validateOtrosRegistros">
                        <a href="#tabParticipante" class="top-tab" aria-controls="tabParticipante" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabParticipante)</a>
                    </li>
                    <li role="presentation" class="validateOtrosRegistros">
                        <a href="#tabAnexo" class="top-tab" aria-controls="tabAnexo" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabAnexos)</a>
                    </li>
                    <li role="presentation" class="validateOtrosRegistros">
                        <a href="#tabFoto" class="top-tab" aria-controls="tabFoto" role="tab" data-toggle="tab"><i class="fa fa-exclamation-circle fa-fn hidden"></i>@Html.Raw(OtrosRegistrosResource.EtiquetaTabFotos)</a>
                    </li>
                </ul>
            </div>
            <div class="right-tabs">
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane fade in active" id="tabGeneral">
                        <form onsubmit="return false;" id="frmInformacionGeneral">
                            <fieldset id="flsInformacionGeneral" @(Model.OtrosRegistros.IndicadorEsLectura == true ? "disabled=disabled" : null)>
                                <div class="title-interna">
                                    <span>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloInformacionGeneral)</span>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaTipoRegistro): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistros.CodigoTipoRegistro, Model.ListaTipoRegistro, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcTipoRegistro",@disabled = "disabled" , @class = "form-control", @validable = "required Validar" } : new { id = "slcTipoRegistro", @class = "form-control", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaProyecto): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistros.CodigoProyecto, Model.ListaProyectos, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcProyecto", @class = "form-control", @validable = "required Validar" } : new { id = "slcProyecto", @class = "form-control", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaPais): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistros.CodigoPais, Model.ListaPais, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcPais", @class = "form-control", @validable = "required Validar" } : new { id = "slcPais", @class = "form-control", @validable = "required Validar", })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaEmpresa): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistros.CodigoEmpresaPais, Model.ListaEmpresa, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcEmpresaPais", @class = "form-control", @validable = "required Validar" } : new { id = "slcEmpresaPais", @class = "form-control", @validable = "required Validar", })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloRegistro): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.TextBox("txtTituloRegistro", Model.OtrosRegistros.TituloRegistro, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "txtTituloRegistro", @class = "form-control", @validable = "required Validar", @maxlength = "50" } : new { id = "txtTituloRegistro", @class = "form-control", @maxlength = "50", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaFechaEjecucion): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.TextBox("dtpFechaEjecucion", Model.OtrosRegistros.FechaEjecucionString, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "dtpFechaEjecucion", @class = "form-control", @validable = "required Validar" } : new { id = "dtpFechaEjecucion", @class = "form-control", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="row">
                                            <div class="form-group">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaHoraEjecucion): (*)</label>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                @Html.DropDownListFor(model => model.OtrosRegistros.HoraEjecucion, Model.ListaHoras, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcHoraEjecucion", @class = "form-control", @validable = "required Validar" } : new { id = "slcHoraEjecucion", @class = "form-control", @validable = "required Validar" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label>@Html.Raw(OtrosRegistrosResource.EtiquetaMinutoEjecucion): </label>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                @Html.DropDownListFor(model => model.OtrosRegistros.MinutoEjecucion, Model.ListaMinutos, Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? (object)new { id = "slcMinutoEjecucion", @class = "form-control", @validable = "required Validar" } : new { id = "slcMinutoEjecucion", @class = "form-control", @validable = "required Validar" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>

                                <div class="row">
                                </div>

                            </fieldset>
                        </form>
                        @if (Model.OtrosRegistros.IndicadorEsLectura != true)
                        {
                            <div id="divBotonAccion" class="box-acciones-foot text-center">
                                <button class="btn btn-default" id="btnSalirInformacionGeneral">@Html.Raw(GenericoResource.BotonSalir)</button>
                                @if (Model.OtrosRegistros.CodigoOtrosRegistros.HasValue && (Model.OtrosRegistros.CodigoEstadoDocumento != DatosConstantes.ParametrosEstadoDocumento.Anulado))
                                { 
                                    <button class="btn btn-primary" id="btnAnularInformacionGeneral">@Html.Raw(GenericoResource.BotonAnular)</button>   
                                }
                                <button class="btn btn-primary" id="btnGuardarInformacionGeneral">@Html.Raw(GenericoResource.BotonGuardar)</button>
                            </div>
                        }
                    </div>
                    <div role="tabpanel" class="tab-pane fade in" id="tabHallazgo">
                        <form onsubmit="return false;" id="frmHallazgo">
                            <fieldset id="flsHallazgo" @(Model.OtrosRegistros.IndicadorEsLectura == true ? "disabled=disabled" : null)>
                                <div class="title-interna">
                                    <span>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloIngresoHallazgo)</span>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaDescripcionCorta): (*)</label>
                                            @Html.TextArea("txtDescripcionCorta", Model.Hallazgo.DescripcionCorta, Model.Hallazgo.CodigoHallazgo.HasValue ? (object)new { id = "txtDescripcionCorta", @class = "form-control", @rows = "2", @validable = "required Validar", @maxlength = "255", @style = "resize:none;" } : new { id = "txtDescripcionCorta", @class = "form-control", @maxlength = "255", @rows = "2", @validable = "required Validar", @style = "resize:none;" })
                                            <div class="form-group text-right">
                                                <label style="font-size: 11px">@Html.Raw(GenericoResource.EtiquetaMaximoAbreviado) 255 @Html.Raw(GenericoResource.EtiquetaCaracteres)</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaDescripcionLarga): (*)</label>
                                            @Html.TextArea("txtDescripcionLarga", Model.Hallazgo.DescripcionLarga, Model.Hallazgo.CodigoHallazgo.HasValue ? (object)new { id = "txtDescripcionLarga", @class = "form-control", @validable = "required Validar", @maxlength = "2500", @rows = "3", @style = "resize:none;" } : new { id = "txtDescripcionLarga", @class = "form-control", @maxlength = "2500", @rows = "3", @validable = "required Validar", @style = "resize:none;" })
                                            <div class="form-group text-right">
                                                <label style="font-size: 11px">@Html.Raw(GenericoResource.EtiquetaMaximoAbreviado) 2500 @Html.Raw(GenericoResource.EtiquetaCaracteres)</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaNivelRiesgo): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownList("slcNivelRiesgo", Model.ListaNivelRiesgo, new { @class = "form-control", @id = "slcNivelRiesgo", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </fieldset>
                        </form>
                        @if (Model.OtrosRegistros.IndicadorEsLectura != true)
                        {
                            <div class="box-acciones-foot text-center">
                                <button class="btn btn-default" @(Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? "" : "hidden") id="btnLimpiarHallazgo">@Html.Raw(GenericoResource.EtiquetaLimpiar)</button>
                                <button class="btn btn-default" @(Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? "" : "hidden") id="btnSalirHallazgo">@Html.Raw(GenericoResource.BotonSalir)</button>
                                <button class="btn btn-primary" @(Model.OtrosRegistros.CodigoOtrosRegistros.HasValue ? "" : "hidden") id="btnGuardarHallazgo">@Html.Raw(GenericoResource.BotonGuardar)</button>
                            </div>                                          
                        }
                        <div class="table-grilla">
                            <div id="divGrdResultadoHallazgo">
                            </div>
                        </div>
                    </div>
                    <div role="tabpanel" class="tab-pane fade in" id="tabAccion">
                        <form onsubmit="return false;" id="frmAccion">
                            <div id="DivfrmAccion" class="title-interna">
                                <span>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloIngresoAcciones)</span>
                            </div>
                            <fieldset id="flsRevisarHallazgoAccion" disabled="disabled">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaNivelRiesgo):</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownList("slcNivelRiesgoAccion", Model.ListaNivelRiesgo, new { @class = "form-control", @id = "slcNivelRiesgoAccion", @validable = "required Validar" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaDescripcionCorta):</label>
                                            @Html.TextArea("txtDescripcionCortaAccion", Model.Hallazgo.DescripcionCorta, Model.Hallazgo.CodigoHallazgo.HasValue ? (object)new { id = "txtDescripcionCortaAccion", @class = "form-control", @rows = "2", @maxlength = "255", @style = "resize:none;" } : new { id = "txtDescripcionCortaAccion", @class = "form-control", @maxlength = "255", @rows = "2", @style = "resize:none;" })
                                            <div class="form-group text-right">
                                                <label style="font-size: 11px">@Html.Raw(GenericoResource.EtiquetaMaximoAbreviado) 255 @Html.Raw(GenericoResource.EtiquetaCaracteres)</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                        @if (Model.OtrosRegistros.IndicadorEsLectura != true)
                        {
                            <div class="acciones-form text-right" style="border: none">
                                <button id="btnAgregarAccion" class="btn btn-primary">@Html.Raw(OtrosRegistrosResource.EtiquetaAgregarAccion)</button>
                            </div>
                        }
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="table-grilla">
                                    <div id="divGrdResultadoAccion">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div role="tabpanel" class="tab-pane fade in" id="tabParticipante">
                        <form onsubmit="return false;" id="frmParticipante">
                            <fieldset id="flsParticipante" @(Model.OtrosRegistros.IndicadorEsLectura == true ? "disabled=disabled" : null)>
                                <div class="title-interna">
                                    <span>@Html.Raw(OtrosRegistrosResource.EtiquetaTituloIngresoParticipantes)</span>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaTipoParticipante): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    @Html.DropDownListFor(model => model.OtrosRegistrosParticipante.CodigoColaboradorParticipante, Model.ListaTipoParticipante, Model.OtrosRegistrosParticipante.CodigoColaboradorParticipante.HasValue ? (object)new { id = "slcTipoParticipante", @class = "form-control", @validable = "required Validar" } : new { id = "slcTipoParticipante", @class = "form-control", @validable = "required Validar" })

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>@Html.Raw(OtrosRegistrosResource.EtiquetaNombre): (*)</label>
                                            <div class="row">
                                                <div class="col-sm-12">
